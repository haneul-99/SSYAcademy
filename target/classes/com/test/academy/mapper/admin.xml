<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="admin">
	<select id = "adminmypage" parameterType="String" resultType = "addto">
		select * from tbladmin where adseq = #{adseq}
	</select>
	
	<select id="studentlist" parameterType="String" resultType="sdto">
		select * from tblStudent 
	</select>
	
	<select id="academylist" parameterType="String" resultType="acdto">
		select * from tblAcademy
	</select>
	
	
</mapper>